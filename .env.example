# Example env variables for GitHub OAuth (do NOT commit secrets)
OAUTH_CLIENT_ID=
OAUTH_CLIENT_SECRET=
SITE_URL=http://localhost:3000
OAUTH_HOSTNAME=github.com

# Optional: OpenAI API key for AI post generation
OPENAI_API_KEY=
# Optional: preferred OpenAI model (eg. gpt-4o-mini)
OPENAI_MODEL=
# Optional: GenMini (image) API key
GENMINI_API_KEY=
# Optional: BingAI API key (if available)
BINGAI_API_KEY=
# Optional: GitHub token for committing generated posts (repo scope)
AI_GITHUB_TOKEN=
